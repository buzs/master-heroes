{
	"env": {
		"development": {
			"presets": ["next/babel"]
		},
		"production": {
			"presets": ["next/babel"]
		},
		"test": {
			"presets": [
				["next/babel", {
					"preset-env": {
						"modules": "commonjs"
					}
				}]
			]
		}
	},
	"plugins": [
		[
			"import",
			{
				"libraryName": "antd",
				"style": true
			}
		],
		["styled-components", {
			"ssr": true
		}]
	]
}